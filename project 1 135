//Project 1
//Task A
//This takes in input from a file with integers and adds them upp
#include <iostream> // includes the file iostream
using namespace std; // indicates that we are using the standard namespace

// main function
int main() {

    // initilization of needed variables
    int s = 0;
    int n;
    //loop to go through the each integer in the file and adds them
    while(cin>>n) {
        int a = n; //places value gotten from each loop into a variable
        s += a; // adds each value taken to the the previous number given
    }
    cout << s; // prints final result

    return 0;
}
